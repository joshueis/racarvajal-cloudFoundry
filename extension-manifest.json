{
  "manifestVersion": 1,
  "id": "cloud-foundry-extension",
  "publisher": "IsraelCarvajal",
  "version": "1.0.201",
  "name": "Cloud Foundry extension to push with rolling strategy",
  "description": "Deploy your app to Cloud Foundry with zero downtime using cf cli 7",
  "public": false,
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "contributions": [
    {
      "id": "cloud-foundry-deploy-task",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "CloudFoundryDeploy"
      }
    },
    {
      "id": "cloud-foundry-uups-task",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "CloudFoundryUUPS"
      }
    }
  ],
  "files": [
    {
      "path": "CloudFoundryDeploy"
    },
    {
      "path": "CloudFoundryUUPS"
    }
  ]
}
